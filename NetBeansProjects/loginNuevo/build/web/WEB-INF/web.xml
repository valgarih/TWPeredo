<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletXML_1</servlet-name>
        <servlet-class>paquete.ServletXML_1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Inicio</servlet-name>
        <servlet-class>paquete.Inicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inicio</servlet-name>
        <servlet-class>paquete.inicio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>principal</servlet-name>
        <servlet-class>paquete.principal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>incorrecto</servlet-name>
        <servlet-class>paquete.incorrecto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletXML_1</servlet-name>
        <url-pattern>/ServletXML_1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Inicio</servlet-name>
        <url-pattern>/Inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>inicio</servlet-name>
        <url-pattern>/inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>principal</servlet-name>
        <url-pattern>/principal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>incorrecto</servlet-name>
        <url-pattern>/incorrecto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
